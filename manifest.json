{
    "manifest_version": 3,
    "name": "HelloBraille",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["styles.css"]
        }
    ],
    "web_accessible_resources": [
        {
          "resources": ["SOV_braille.ttf", "SOV_braille_only.ttf"],
          "matches": ["<all_urls>"]
        }
    ]
}